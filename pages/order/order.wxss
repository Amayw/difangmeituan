page{
  width: 100%;
  background-color: #f0f0f0;
  height:100%;
}

.tab{
  background-color: #fff;
  width: 92%;
  /* height: 300px; */
  border: 1px solid #fff;
  border-radius: 10px;
  margin:10px auto;
  color:#6d6a6a;
  box-shadow:rgb(233, 228, 228) 0px 0px 2px 5px ;
}

.tab-title{
  width:100%;
  height:80rpx;
  line-height:80rpx;
  display:flex;
  flex-direction: row;
  background-color: #F8F8F8;
  font-size: 40rpx;
  color: #6d6a6a;
}
.tab-title view{
  flex:auto;
  text-align:center;
}
.active{
  background-color:#fff;
  color: #333;
}

.show{
  display:block;
  }
.hidden{display:none;}

.selected1{
  display: flex;
  height:200px;
  color:#6d6a6a;
}
.selected2{
  display: flex;
  height:300px;
  color:#6d6a6a;
}
.tab-content{
  width: 100%;
  padding-left:10rpx;
}
.tab-content .itemcontent1{
  margin:10px;
  font-size:35rpx;
  color: #6d6a6a;
}
.itemli1{
  margin:10px;
  font-size:40rpx;
  color:#222;
   text-overflow: -o-ellipsis-lastline;/*css3中webkit内核提供的一个方法类似ellipsis*/
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;/*自适应盒子*/
  -webkit-line-clamp: 2;/*此处为1行,如果是两行就改成2*/
  -webkit-box-orient: vertical;
  width:92%;
  padding-bottom:5px;
  border-bottom:1px solid #eee;
}
.tab-content .itemcontent2,.itemcontent3,.itemcontent4,.itemcontent5{
  width: 92%;
  display: flex;
  margin:3px 10px;
  font-size:30rpx;
  color: #6d6a6a;
  padding-bottom:5px;
  border-bottom:1px solid #eee;
}
.itemcontent5{
  border-bottom:none;
}
.tab-content .itemcontent2 .itemli21,.itemli22{
  display: flex;
  flex:1;
  flex-direction: column;
  color: #6d6a6a;
}
.tab-content .itemcontent2 .itemli22{
  border-left:1px solid #eee;
  padding-left:30rpx;
}
.tab-content .itemcontent2 .itemli211,.itemli212,.itemli221,.itemli222{
  display: flex;
  flex:1;
  color: #6d6a6a;
}
.tab-content .itemcontent2 .itemli212,.itemli222{
  display: flex;
  flex:1;
  color: #5c5959;
}

.tab-content .itemcontentagree{
  width: 92%;
  display: flex;
  margin:10px;
  font-size:30rpx;
  color: #6d6a6a;
}

/*checkbox 选项框大小  */
checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
}
/*checkbox选中后样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border:1px solid #4993F7;
  color:#4993F7;
}
checkbox text{
  color:#3967AA;
}

